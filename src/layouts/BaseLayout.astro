---
import Header from "../components/Header.astro";
import Carousel from "../components/Carousel.astro";
import List from "../components/List.astro";
import ContactUs from "../components/ContactUs.astro";
const { pageTitle } = Astro.props;
---

<html>
  <head>
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta 
      charset="utf-8"
      name="description"
      content="Termelői méz vásárolható személyesen Szentpéteri András ellenőrzött termelőtől. Akácméz, repceméz, napraforgó méz és virágpor többféle kiszerelésben eladó. Különleges ajándék csomagolással is szolgálunk."
    <title>{pageTitle}</title>
  </head>

  <body>
    <Header />
    <Carousel />
    <List />
    <ContactUs/>
  </body>
</html>
